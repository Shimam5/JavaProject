pipeline{
	agent any
	stages{
		stage('SCM CheckOut'){
			steps{
				echo "Download the code from git"
			}			
		}
		stage('Compile'){
			steps{
				bat 'mvn clean compile'
			}
		}
		stage('Test'){
			steps{
				bat 'mvn test'
			}
		}
		stage('Build'){
			steps{
				bat 'mvn install'
			}
		}
		stage('Deploy'){
			steps{
				echo "Deploy into the maven repo"
			}
		}
	}	
}